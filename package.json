    "eslint": "^3.19.0",
    "pug-lint-config-emarsys": "2.0.0",
    "mocha": "2.3.4",
    "pug-lint": "^2.5.0"
  },
  "peerDependencies": {
    "pug-lint": ">= 2 < 3"
  }
